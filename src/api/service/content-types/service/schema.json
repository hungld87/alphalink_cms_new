{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "thumb": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "service_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-category.service-category",
      "inversedBy": "services"
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "hot_info": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hot-info.hot-info",
      "inversedBy": "services"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "integer"
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "team_size": {
      "type": "integer",
      "required": true
    },
    "support_duration": {
      "type": "string",
      "required": true
    },
    "is_featured": {
      "type": "boolean",
      "required": false,
      "default": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "features": {
      "type": "component",
      "component": "block.feature-item",
      "repeatable": true
    },
    "benefits": {
      "type": "component",
      "component": "block.benefit-item",
      "repeatable": true
    },
    "process": {
      "type": "component",
      "component": "block.process-item",
      "repeatable": true
    },
    "faq": {
      "type": "component",
      "component": "block.faq-item",
      "repeatable": true
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service-category.service-category"
    },
    "back_ground": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
